{
  "name": "eulercopilot-desktop",
  "private": true,
  "version": "0.9.2-alpha",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri:dev": "RUST_BACKTRACE=1 tauri dev",
    "tauri:release": "TAURI_TRAY=gtk tauri build",
    "cargo:format": "cd src-tauri && cargo fmt",
    "cargo:check": "cd src-tauri && cargo check",
    "cargo:clean": "cd src-tauri && cargo clean",
    "update": "pnpm update && cd src-tauri && cargo update"
  },
  "dependencies": {
    "@computing/opendesign2": "file:lib/opendesign2-2.0.11.tgz",
    "@tauri-apps/api": "^1.6.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "2.3.12",
    "highlight.js": "^11.11.1",
    "marked": "^14.1.4",
    "marked-highlight": "^2.2.1",
    "pinia": "^2.3.1",
    "tauri-plugin-autostart-api": "github:tauri-apps/tauri-plugin-autostart#v1",
    "tauri-plugin-context-menu": "^0.8.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vue-tsc": "^2.2.2"
  }
